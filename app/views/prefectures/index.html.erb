<main class="main">
  <div class="inner">
    <div class="card__wrapper">
      <% @posts.each do |post|%>
        <%= post.title %>
        <%= image_tag post.image, size: "100x100" %>
      <% end %>
    </div>
  </div>
</main>
<%= link_to(new_post_path, class: 'post-btn') do %>
  <span class='post-btn-text'>投稿する</span>
  <%= image_tag 'icon_camera.png' , size: '185x50' ,class: "post-btn-icon" %>
<% end %>
