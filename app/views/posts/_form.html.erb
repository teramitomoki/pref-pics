<%= form_with model: post, local: true do |f|%>

  <div class="field">
    <%= f.label :image, "投稿画像" %><br />
    <%= f.file_field :image, id:"prototype_image" %>
  </div>
  <div>
    <%= f.label :title, "都道府県選択" %><br />
    <%= f.collection_select(:prefecture_id, Prefecture.all, :id, :name, {}, {class:"genre-select"}) %>
  </div>
  <div>
    <%= f.label :title, "ジャンル選択" %><br />
    <%= f.collection_select(:genre_id, Genre.all, :id, :name, {}, {class:"genre-select"}) %>
  </div>
  <div class="field">
    <%= f.label :title, "タイトル(任意)" %><br />
    <%= f.text_field :title, id:"prototype_title" %>
  </div>



  <div class="actions">
    <%= f.submit "投稿する", class: :form__btn  %>
  </div>
<% end %>